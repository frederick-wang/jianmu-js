{
  "name": "jianmu",
  "version": "0.0.4",
  "description": "A node.js package for Jianmu Framework, includeing API calls and CLI tools.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jianmu-js": "scripts/cli.js"
  },
  "files": [
    "config",
    "dist",
    "electron",
    "scripts",
    "tsconfig-preset-ui.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./api": {
      "require": "./dist/api.js",
      "import": "./dist/api.mjs",
      "default": "./dist/api.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frederick-wang/node-jianmu.git"
  },
  "keywords": [
    "Jianmu",
    "Python",
    "Vue",
    "Electron"
  ],
  "author": "Zhaoji Wang <hwoam@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/frederick-wang/node-jianmu/issues"
  },
  "homepage": "https://github.com/frederick-wang/node-jianmu#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli": "^5.0.4",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "cross-env": "^7.0.3",
    "electron": "^19.0.3",
    "electron-builder": "^23.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.9",
    "vite-plugin-html": "^3.2.0",
    "vue-eslint-parser": "^9.0.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "element-plus": "^2.2.4",
    "got": "^11.8.5",
    "hotkeys-js": "^3.9.4",
    "ky": "^0.30.0",
    "vue": "^3.2.36"
  }
}
