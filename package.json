{
  "name": "jianmu",
  "version": "0.0.10",
  "description": "A node.js package for Jianmu Framework, includeing API calls and CLI tools.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jianmu-js": "scripts/cli.js"
  },
  "files": [
    "config",
    "dist",
    "electron",
    "scripts",
    "tsconfig-preset-ui.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./api": {
      "require": "./dist/api.js",
      "import": "./dist/api.mjs",
      "default": "./dist/api.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frederick-wang/jianmu-js.git"
  },
  "keywords": [
    "Jianmu",
    "Python",
    "Vue",
    "Electron"
  ],
  "author": "Zhaoji Wang <hwoam@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/frederick-wang/jianmu-js/issues"
  },
  "homepage": "https://github.com/frederick-wang/jianmu-js#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "tsup": "^6.6.0",
    "vue-eslint-parser": "^9.0.2"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vue/cli": "^5.0.4",
    "autoprefixer": "^10.4.13",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "cross-env": "^7.0.3",
    "del": "^6.1.1",
    "electron": "^23.0.0",
    "element-plus": "^2.2.29",
    "got": "^11.8.5",
    "hotkeys-js": "^3.9.4",
    "install": "^0.13.0",
    "npm": "^9.2.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.58.0",
    "socket.io-client": "^4.6.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^2.9.15",
    "vite-plugin-html": "^3.2.0",
    "vue": "^3.2.47"
  }
}
