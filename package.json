{
  "name": "jianmu",
  "version": "0.0.1",
  "description": "A node.js package for Jianmu Framework, includeing API calls and CLI tools.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./api": {
      "require": "./dist/api.js",
      "import": "./dist/api.mjs",
      "default": "./dist/api.js"
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frederick-wang/node-jianmu.git"
  },
  "keywords": [
    "Jianmu",
    "Python",
    "Vue",
    "Electron"
  ],
  "author": "Zhaoji Wang <hwoam@outlook.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/frederick-wang/node-jianmu/issues"
  },
  "homepage": "https://github.com/frederick-wang/node-jianmu#readme",
  "devDependencies": {
    "electron": "^19.0.4",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "element-plus": "^2.2.5",
    "vue": "^3.2.37"
  }
}
